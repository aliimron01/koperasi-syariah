<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >

        <record id="seq_permohonan_pinjaman" model="ir.sequence">
            <field name="name">Sequence Permohonan Pinjaman</field>
            <field name="code">seq.permohonan.pinjaman</field>
            <field name="prefix">PER</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_pembayaran_angsuran" model="ir.sequence">
            <field name="name">Sequence Pembayaran Angsuran</field>
            <field name="code">seq.pembayaran.angsuran</field>
            <field name="prefix">ANG</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
